  "plugins": [
    "https://plugins.dprint.dev/typescript-0.89.1.wasm",
    "https://plugins.dprint.dev/json-0.19.1.wasm",
    "https://plugins.dprint.dev/markdown-0.16.3.wasm"
  ]
}
